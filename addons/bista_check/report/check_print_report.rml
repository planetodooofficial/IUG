<?xml version="1.0"?>
<document filename="Sales Order.pdf">
    <template title="Sales Order" author="OpenERP S.A.(sales@openerp.com)" allowSplitting="20">
        <pageTemplate id="first">
            <frame id="first" x1="15.0" y1="15.0" width="539" height="770"/>

            <pageGraphics>
                <image x="0.8cm" y="27.9cm" height="44.0" >[[ company.logo or removeParentNode('image') ]]</image>
<!--                <image x="0.8cm" y="27.3cm" height="60.0" >[[ company.logo or removeParentNode('image') ]]</image>-->
                <stroke color="#b51956"/>
                <fill color="#b51956"/>
                <lines>0.8cm 27.8cm 8.2cm 27.8cm</lines>
                <setFont name="Helvetica" size="6"/>
                <drawString x="0.9cm" y="27.5cm">Office Location:</drawString>
                <drawString x="0.9cm" y="27.2cm">Mailing Address: </drawString>
                <drawString x="0.9cm" y="26.9cm">Tel:</drawString>
                <drawString x="0.9cm" y="26.6cm">Fax:</drawString>
                <fill color="black"/>
                <drawString x="2.5cm" y="27.5cm">[[ company.partner_id.street]],[[ company.partner_id.street2]],[[ company.partner_id.zip]] </drawString>
                <drawString x="2.6cm" y="27.2cm">[[ company.partner_id.city]],[[ company.partner_id.state_id.name]],[[ company.partner_id.country_id.name]]</drawString>
                <drawString x="1.4cm" y="26.9cm">[[ company.partner_id.phone]]</drawString>
                <drawString x="1.4cm" y="26.6cm">[[ company.partner_id.fax]]</drawString>

                <setFont name="Helvetica-Bold" size="11"/>
                <!--drawCentredString x="12cm" y="28.1cm">Sales Order Confirmation</drawCentredString>
                <setFont name="Helvetica-Bold" size="10"/>
                <drawCentredString x="12cm" y="27.6cm">Sales Order: [[o.name]] [[o.origin]]</drawCentredString>
                <drawCentredString x="18cm" y="27.6cm">Date Order: [[ formatLang(o.date_order,date = True) ]]</drawCentredString-->
                <setFont size="8" name="Helvetica"/>
                <fill color="black"/>
                <stroke color="black"/>
<!--                <drawCentredString x="15.4cm" y="4.8cm"> <pageNumber/></drawCentredString>-->
                <drawCentredString x="18.7cm" y="28.8cm"> <pageCount/> </drawCentredString>
            </pageGraphics>

        </pageTemplate>
    </template>
