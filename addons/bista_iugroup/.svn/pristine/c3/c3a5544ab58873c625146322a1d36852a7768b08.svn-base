        <record id="view_zip_code_form" model="ir.ui.view">
            <field name="name">Zip Code</field>
            <field name="model">zip.code</field>
            <field name="arch" type="xml">
                <form string="Zip Code" version="7.0">
                    <sheet>
                        <group>
                            <h3><field name="name"/></h3>
                        </group>

                        <group colspan="4" col="6">
                            <field name="city" />
                            <field name="state_id" />
                            <field name="time_zone" />
                            <field name="latitude" />
                            <field name="longitude" />
                            <field name="latitude_rad" />
                            <field name="longtitude_rad" />
                            <field name="longitude" />
                            <field name="zip_code_id" />
                            <field name="company_id" groups="base.group_multi_company" widget="selection"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="view_zip_code_tree" model="ir.ui.view">
            <field name="name">Zip Code</field>
            <field name="model">zip.code</field>
            <field name="arch" type="xml">
                <tree string="Zip Code">
                    <field name="name"/>
                    <field name="city" />
                    <field name="state_id" />
                    <field name="time_zone" />
                    <field name="latitude" />
                    <field name="longitude" />
                    <field name="latitude_rad" />
                    <field name="longtitude_rad" />
                    <field name="longitude" />
                    <field name="zip_code_id" readonly="1"/>
                    <field name="company_id" groups="base.group_multi_company" widget="selection"/>

                </tree>
            </field>
        </record>

        <record id="action_zip_code_form" model="ir.actions.act_window">
            <field name="name">Zip Code</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">zip.code</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_zip_code_tree"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to add a Zip Code
                </p><p>
                Odoo helps you easily track all activities related to
                a Zip Code discussions, history of business Zip Code etc.
            </p>
            </field>
        </record>

