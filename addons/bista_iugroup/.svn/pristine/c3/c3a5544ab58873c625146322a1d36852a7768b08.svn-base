        <record id="view_event_outcome_form" model="ir.ui.view">
            <field name="name">Event Out Come</field>
            <field name="model">event.out.come</field>
            <field name="arch" type="xml">
                <form string="Event Outcome" version="7.0">
                    <sheet>
                        <group>
