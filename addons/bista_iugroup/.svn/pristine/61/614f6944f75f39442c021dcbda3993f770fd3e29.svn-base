       <record id="invoice_tree_iu" model="ir.ui.view">
            <field name="name">account.invoice.tree</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_tree"/>
            <field name="arch" type="xml">
                <xpath expr="/tree" position="attributes">
                    <attribute name="decoration-info">"is_emailed == True or is_monthly == True or is_mailed == True or is_printed == True or is_faxed == True"</attribute>
                </xpath>
                <xpath expr="/tree/field[@name='date_invoice']" position="replace">
                    <field name="date_invoice" invisible="1"/>
                    <field name="event_start_date" string="Date of Service"/>
                    <field name="patient_id" string="Patient Name"/>
                    <field name="project_name_id" string="Project"/>
                    <field name="ref" string="Ref"/>
                    <field name="language_id" />
                    <field name="is_emailed" invisible="1"/>
                    <field name="is_mailed" invisible="1"/>
                    <field name="is_monthly" invisible="1"/>
                    <field name="is_faxed" invisible="1"/>
                    <field name="is_printed" invisible="1"/>
                </xpath>
                <xpath expr="/tree/field[@name='user_id']" position="replace">
                    <field name="user_id" invisible="1"/>
                </xpath>
            </field>
        </record>
        
        <!--Added this record to add event date in move line-->
        <record id="view_move_form_iug" model="ir.ui.view">
            <field name="name">account.move.form.iug</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='line_ids']/tree/field[@name='date_maturity']" position="after">
                    <field name="event_date" string="Date of Service"/>
                    <field name="patient_id" string="Patient"/>
                    <field name="project_name_id" string="Project"/>
                    <field name="reference" string="Reference"/>
                </xpath>
            </field>
        </record>
       <record id="view_invoice_iu_inherit" model="ir.ui.view">
            <field name="name">view.invoice.iu.inherit.view</field>
            <field name="model">account.invoice</field>
            <field name="type">form</field>
            <field name="priority" eval="20"/>
            <field name="inherit_id" ref="account.invoice_form"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="/form/header/button[@name='invoice_print']" position="replace"/>
                    <xpath expr="/form/header" position="inside">
                        <style>.o_form_sheet{max-width:90% !important}</style>
                        <button name="reset_to_draft_iu" string="Reset To Draft" type="object" states="open" groups="account.group_account_invoice"/>
                        <!--script buttons commented-->
                        <!--<button name="correct_move_date_period" string="Correct Move Date Period" type="object" groups="bista_iugroup.group_iu_admin"/>-->
                        
                        <!--<button name="correct_translation_invoices" string="Correct Translation Invoices" type="object" groups="bista_iugroup.group_iu_admin"/>-->
                        <!--<button name="post_invoice" string="Post Invoice" type="object" groups="bista_iugroup.group_iu_admin"/>
                        <button name="reset_to_draft" string="Reset To Draft" type="object" groups="bista_iugroup.group_iu_admin"/>
                        <button name="set_amount_due" string="Set Amount Due" type="object" groups="bista_iugroup.group_iu_admin"/>-->
                    </xpath>
                    <xpath expr="/form/sheet/notebook" position="inside">
                         <page string="Event Info">
                            <group colspan="3" col="6" readonly="1">
                                <field name="event_id" context="{'form_view_ref':'bista_iugroup.view_event_form'}" readonly="1"/>
                                <button name="get_event_view" string="View Event" type="object" groups="account.group_account_invoice"/>
                                <separator></separator>
<!--                                <field name="interpreter_id" domain="[('cust_type','in',('interpreter','interp_and_transl'))]"
                                       context="{'form_view_ref':'bista_iugroup.view_interpreter_form'}"
                                       options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" />-->
<!--                                <field name="transporter_id" context="{'form_view_ref':'bista_iugroup.view_transporter_form'}"
                                       domain="[('cust_type','in',('transporter'))]"
                                       options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" />
                                <field name="translator_id" context="{'form_view_ref':'bista_iugroup.view_translator_form'}"
                                       domain="[('cust_type','in',('translator','interp_and_transl'))]"
                                       options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" />-->
                                <field name="doctor_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" readonly="1"/>
                                <field name="location_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" readonly="1"/>
                                <field name="event_start_date"/>
                                <field name="event_start"/>
                                <field name="patient_id"/>
                                <field name="is_monthly" />
                                <field name="is_mailed" />
                                <field name="is_emailed" />
                                <field name="is_faxed" />
                                <field name="is_printed" />
                                <field name="quickbooks_id" />
                           </group>
                           <group>
                               <field name="language_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" 
                                      domain="[('company_id','=',company_id]"/>
                           </group>
                     </page>
                </xpath>
                <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='account_analytic_id']" position="replace"/>

                <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='quantity']" position="replace">
                     <field name="quantity" string="HR/Miles"/>
                </xpath>
                <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='price_subtotal']" position="after">
                     <field name="total_editable" sum="total_editable" string="Edit Total"/>
                </xpath>
                <xpath expr="//field[@name='date_due']" position="replace">
                     <field name="date_due" attrs="{'readonly':[('state','in',('open','paid','cancelled'))]}"/>
                </xpath>
                <!--<xpath expr="/form/sheet/notebook/page[@string='Payments']/field[@name='payment_ids']/tree/field[@name='name']" position="after">-->
                     <!--<field name="check_number"/>-->
                <!--</xpath>-->
                <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='invoice_line_tax_ids']" position="replace">
                    <field name="invoice_line_tax_ids" widget="many2many_tags" context="{'type':parent.type}"
                                        domain="[('parent_id','=',False),('type_tax_use','=','sale'),('company_id', '=', parent.company_id)]" options="{'no_create': True}" invisible="1"/>
                </xpath>
<!--                 <xpath expr="/form/sheet/notebook/page[@string='Invoice Lines']/field[@name='invoice_line']/tree/field[@name='account_id']" position="replace">
                     <field name="account_id" groups="account.group_account_user" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"
                            domain="[('company_id', '=', parent.company_id), ('journal_id', '=', parent.journal_id), ('type', '=', 'other')]"
                            on_change="onchange_account_id(product_id, parent.partner_id, parent.type, parent.fiscal_position,account_id)" invisible="1"/>
                </xpath>-->

                <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='price_unit']" position="replace">
<!--                     <field name="actual_time"  invisible="1"/>-->
                     <field name="price_unit" string="Rate"/>
                     <field name="travel_time" />
                     <field name="travelling_rate" />
                     <field name="miles_driven"/>
                     <field name="mileage"/>
                     <field name="mileage_rate"/>
                     <field name="miscellaneous_bill" invisible="1"/>
<!--                     <field name="discount" groups="sale.group_discount_per_so_line"/>-->
                </xpath>

                <xpath expr="//field[@name='partner_id']" position="replace">
                    <field string="Customer" name="partner_id"
                           context="{'default_customer': 1, 'search_default_customer': 1, 'default_supplier': 0, 'show_address': 1,'form_view_ref':'bista_iugroup.view_partner_form_inherit_iu'}"
                           domain="[('customer', '=', 1),('cust_type', '=', 'customer'),('company_id', '=',company_id)]" options='{"always_reload": True}'/>
                    <field name="cust_gpuid" attrs="{'invisible': ['|',('cust_gpuid','=',False)]}" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                    <field name="cust_csid" attrs="{'invisible': ['|',('cust_csid','=',False)]}"/>
                    <field name="contact_id" domain="[('customer', '=', 1),('cust_type', '=', 'contact'),('company_id','=',company_id),('parent_id','=',partner_id )]"
                           options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" context="{ 'form_view_ref':'bista_iugroup.view_contact_form'}"/>
                    <field name="check_no" readonly="0"/>
                </xpath>
                <xpath expr="//field[@name='date_invoice']" position="before">
                    <field name="ordering_partner_id" domain="[('customer', '=', 1),('cust_type', '=', 'customer'),('company_id', '=',company_id)]"
                           options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"
                           context="{'default_customer': 0, 'search_default_supplier': 1, 'default_supplier': 1, 'show_address': 1,'form_view_ref':'bista_iugroup.view_partner_form_inherit_iu'}" />
                    <field name="ordering_contact_id" domain="[('customer', '=', 1),('cust_type', '=', 'contact'),('company_id','=',company_id),('parent_id','=',ordering_partner_id )]"
                           options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" context="{ 'form_view_ref':'bista_iugroup.view_contact_form'}"/>
                </xpath>
                <xpath expr="//field[@name='date_invoice']" position="attributes">
                    <attribute name="attrs">{'readonly':[('state','in',('open','paid','cancelled'))]}</attribute>
                 </xpath>
                <xpath expr="/form/sheet/group" position="after">
                    <group colspan="4">
                        <h3>
                            <field name="invoice_ref" context="{'form_view_ref':'account.invoice_supplier_form'}"/>
                        </h3>
                    </group>
                </xpath>
                <xpath expr="//field[@name='comment']" position="after">
                    <div class="oe_clear">
                        <label for="internal_comment"/>
                    </div>
                    <field name="internal_comment" class="oe_inline" placeholder="Internal notes..."/>
                </xpath>
                </data>
            </field>
        </record>
        
        <record id="account.invoice_supplier_form" model="ir.ui.view">
            <field name="name">account.invoice.supplier.form</field>
            <field name="model">account.invoice</field>
            <field name="priority">2</field>
            <field name="arch" type="xml">
              <form string="Vendor Invoice" version="7.0">
                <header>
                    <button name="action_invoice_open" states="draft,proforma2" type="object" string="Validate" class="oe_highlight" groups="account.group_account_invoice"/>
                    <button name="%(account.action_account_invoice_payment)d" type="action" states="open" string="Register Payment" groups="account.group_account_invoice" class="oe_highlight"/>
                    <button name="%(account.action_account_invoice_refund)d" type='action' string='Ask Refund' states='open,paid' groups="account.group_account_invoice"/>
                    <button name="action_invoice_cancel" states="draft,proforma2" string="Cancel Invoice" groups="account.group_account_invoice" type="object"/>
                    <button name="action_cancel_draft" states="cancel" string="Set to Draft" type="object" groups="account.group_account_invoice"/>

                    <field name="state" widget="statusbar" statusbar_visible="draft,open,paid" statusbar_colors='{"proforma":"blue","proforma2":"blue"}'/>
                    <button name="reset_to_draft_iu" string="Reset To Draft" type="object" states="open" groups="account.group_account_invoice"/>
                                            <!--script buttons commented-->
                    
                    <!--<button name="post_invoice_supplier" string="Post Invoice" type="object" groups="bista_iugroup.group_iu_admin"/>
                    <button name="reset_to_draft" string="Reset To Draft" type="object" groups="bista_iugroup.group_iu_admin"/>-->
                </header>
                <sheet string="Vendor Invoice">
                    <style>.o_form_sheet{max-width:90% !important}</style>
                    <div class="oe_title">
                        <h1>
                            <label string="Draft Invoice" attrs="{'invisible': ['|',('state','&lt;&gt;','draft'), ('type','&lt;&gt;','in_invoice')]}"/>
                            <label string="Draft Refund" attrs="{'invisible': ['|',('state','&lt;&gt;','draft'), ('type','&lt;&gt;','in_refund')]}"/>
                            <label string="Invoice" attrs="{'invisible': ['|',('state', '=', 'draft'), ('type','&lt;&gt;','in_invoice')]}"/>
                            <label string="Refund" attrs="{'invisible': ['|',('state', '=', 'draft'), ('type','&lt;&gt;','in_refund')]}"/>
                            <field name="number" class="oe_inline"  attrs="{'invisible': [('state', '=', 'draft')]}"/>
                        </h1>
                    </div>
                    <field name="type" invisible="1"/>
                    <group>
                        <group>
                            <field string="Vendor" name="partner_id"

                                   context="{'default_customer': 0, 'search_default_supplier': 1, 'default_supplier': 1, 'show_address': 1,'form_view_ref':'bista_iugroup.view_interpreter_form'}"
                                   domain="[('supplier', '=',1),('customer','=',0),('company_id', '=',company_id)]"
                                   options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                            <newline/>
                            <field name="cust_gpuid" attrs="{'invisible': ['|',('cust_gpuid','=',False)]}"/>
                            <field name="cust_csid" attrs="{'invisible': ['|',('cust_csid','=',False)]}"/>
                            <field name="contact_id" domain="[('customer', '=', 1),('cust_type', '=', 'contact'),('company_id','=',company_id),('parent_id','=',partner_id )]"
                                   options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" context="{ 'form_view_ref':'bista_iugroup.view_contact_form'}"/>
                            <field name="fiscal_position_id" widget="selection"/>
                            <field name="origin"/>
                            <field name="reference" string="Vendor Reference"/>
                        </group>
                        <group>
                            <field name="date_invoice" attrs="{'readonly':[('state','in',('open','paid','cancelled'))]}" />
                            <field name="ordering_partner_id" domain="[('customer', '=', 1),('cust_type', '=', 'customer'),('company_id', '=',company_id)]"
                                   options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"
                                   context="{'default_customer': 0, 'search_default_supplier': 1, 'default_supplier': 1, 'show_address': 1,'form_view_ref':'bista_iugroup.view_partner_form_inherit_iu'}"/>
                            <field name="ordering_contact_id" domain="[('customer', '=', 1),('cust_type', '=', 'contact'),('company_id','=',company_id),('parent_id','=',ordering_partner_id )]"
                                   options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" context="{'form_view_ref':'bista_iugroup.view_contact_form'}"/>
                            <field name="date_due" attrs="{'readonly':[('state','in',('open','paid','cancelled'))]}"/>
                            <field domain="[('company_id', '=', company_id), ('type', '=', 'payable')]"
                                   name="account_id" groups="account.group_account_user"/>
                            <field name="journal_id" groups="account.group_account_user"
                                    widget="selection"/>
                            <field name="currency_id" groups="base.group_multi_currency"/>
                        </group>
                    </group>
                    <h3>
                        <group colspan="4" col="4" >
                            <group>
                                <field name="invoice_ref" context="{'form_view_ref':'view_invoice_iu_inherit'}"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </h3>
                    <notebook>
                        <page string="Invoice">
                            <field context="{'partner_id': partner_id, 'price_type': context.get('price_type') or False, 'type': type}" name="invoice_line_ids">
                                <tree string="Invoice lines" editable="bottom">
                                    <field name="sequence" widget="handle" />
                                    <field name="product_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"
                                           />
                                    <field name="name"/>
                                    <field name="company_id" invisible="1"/>
                                    <field name="account_id" groups="account.group_account_user" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"
                                           domain="[('company_id', '=', parent.company_id), ('journal_id', '=', parent.journal_id), ('type', '=', 'other')]"
                                            />
                                    <field name="account_analytic_id" groups="analytic.group_analytic_accounting"
                                           domain="[('type','!=','view'), ('company_id', '=', parent.company_id)]" invisible="1"/>
                                    <field name="quantity" string="HR/Miles"/>
                                    <field name="uom_id" groups="product.group_uom" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"
                                           />
                                    <field name="price_unit" string="Rate"/>
                                    <field name="travel_time" />
                                    <field name="travelling_rate" />
                                    <field name="miles_driven"/>
                                    <field name="mileage"/>
                                    <field name="mileage_rate"/>
                                    <field name="miscellaneous_bill" invisible="1"/>
                                    <field name="discount" groups="sale.group_discount_per_so_line"/>
                                    <field name="invoice_line_tax_ids" widget="many2many_tags" context="{'type':parent.type}"
                                           domain="[('parent_id','=',False),('company_id', '=', parent.company_id)]" invisible="1"/>
                                    <field name="price_subtotal"/>
                                    <field name="total_editable" sum="total_editable" string="Edit Total"/>
                                </tree>
                            </field>
                            <group class="oe_subtotal_footer oe_right">
                                <field name="amount_untaxed" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                                <div>
                                    <label for="amount_tax"/>
                                    <button name="button_reset_taxes" states="draft,proforma2"
                                        string="(update)" class="oe_link oe_edit_only"
                                        type="object" help="Recompute taxes and total"/>
                                </div>
                                <field name="amount_tax" nolabel="1" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                                <field name="amount_total" class="oe_subtotal_footer_separator" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                                <field name="residual" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                                <field name="reconciled" invisible="1"/>
                            </group>
                            <div style="width:50%">
                                <field name="tax_line_ids">
                                    <tree editable="bottom" string="Taxes">
                                        <field name="name"/>
                                        <field name="account_id" groups="account.group_account_invoice"/>
                                        <field name="account_analytic_id" domain="[('type','&lt;&gt;','view'), ('company_id', '=', parent.company_id)]" groups="analytic.group_analytic_accounting"/>
                                        <field name="amount"/>
                                    </tree>
                                </field>
                            </div>
                            <div class="oe_clear">
                                <label for="comment"/>
                            </div>
                            <field name="comment" nolabel="1" colspan="4" widget="html"/>
                            <div class="oe_clear">
                                <label for="internal_comment"/>
                            </div>
                            <field name="internal_comment" class="oe_inline" placeholder="Internal notes..."/>
                        </page>
                        <page string="Other Info">
                            <group>
                                <group>
                                    <field domain="[('partner_id', '=', partner_id)]" name="partner_bank_id" />
                                    <field name="user_id" domain="[('company_id', '=', company_id)]" options="{'limit': 7, 'create': false, 'create_edit': false, 'no_open': True}"/>
                                    <field name="sales_representative_id"/>
                                    <field name="name" invisible="1"/>
                                    <field name="payment_term_id" widget="selection"/>
                                    <field name="state_name_related" invisible="1"/>
                                </group>
                                <group>
                                    <field name="move_id" groups="account.group_account_user"/>
                                    <field name="period_id" domain="[('state', '=', 'draft'), ('company_id', '=', company_id)]" groups="account.group_account_user"/>
                                    <field name="company_id" />
                                </group>
                            </group>
                        </page>
                        <!--<page string="Payments">-->
                            <!--<field name="payment_ids">-->
                                <!--<tree string="Payments">-->
                                    <!--<field name="date" string="Payment Date"/>-->
                                    <!--<field name="move_id"/>-->
                                    <!--<field name="ref"/>-->
                                    <!--<field name="name"/>-->
                                    <!--<field name="check_number" />-->
                                    <!--<field name="journal_id"/>-->
                                    <!--<field name="debit"/>-->
                                    <!--<field name="credit"/>-->
                                    <!--<field name="amount_currency"/>-->
                                    <!--<field name="currency_id" groups="base.group_multi_currency"/>-->
                                <!--</tree>-->
                            <!--</field>-->
                        <!--</page>-->
                        <page string="Event Info" >
                            <group colspan="3" col="6" readonly="1">
                                <field name="event_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"
                                       context="{'form_view_ref':'bista_iugroup.view_event_form'}" readonly="1"/>
                                <field name="project_name_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
<!--                                <field name="transporter_id" context="{'form_view_ref':'bista_iugroup.view_transporter_form'}"
                                       domain="[('cust_type','in',('transporter'))]"
                                       options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" />
                                <field name="translator_id" context="{'form_view_ref':'bista_iugroup.view_translator_form'}"
