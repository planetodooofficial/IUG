                                       domain="[('cust_type','in',('translator','interp_and_transl'))]"
                                       options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" />-->
                                <field name="doctor_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" />
                                <field name="location_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" />
