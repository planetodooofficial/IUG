      <record id="view_account_invoice_filter_iug" model="ir.ui.view">
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.view_account_invoice_filter"/>
            <field name="arch" type="xml">
                <xpath expr="/search/filter[@name='unpaid']" position="replace">
                    <field name="language_id"/>
                    <filter name="unpaid" string="Open" domain="[('state','=','open')]" help="Open Invoices"/>
                    <filter name="paid" string="Paid" domain="[('state','=','paid')]" help="Paid Invoices"/>
                    <filter name="emailed" string="Emailed" domain="[('is_emailed','=',True)]" help="Emailed Invoices"/>
                    <filter name="mailed" string="Mailed" domain="[('is_mailed','=',True)]" help="Mailed Invoices"/>
                    <filter name="faxed" string="Faxed" domain="[('is_faxed','=',True)]" help="Faxed Invoices"/>
                    <filter name="printed" string="Printed" domain="[('is_printed','=',True)]" help="Printed Invoices"/>
                    <filter name="is_monthly" string="Monthly" domain="[('is_monthly','=',True)]" help="Monthly Invoices"/>
                    <filter name="no_action" string="No Action" help="No Action taken on this invoice"
                            domain="[('is_monthly','=',False),('is_emailed','=',False),('is_mailed','=',False),('is_faxed','=',False),('is_printed','=',False)]" />
                    
                    <filter name="language" string="Language" domain="[('event_type','=','language')]" />
                    <filter name="translation" string="Translation" domain="[('event_type','=','translation')]" />
                    <filter name="transport" string="Transport" domain="[('event_type','=','transport')]" />
                    <filter name="lang_trans" string="Language &amp; Transport" domain="[('event_type','=','lang_trans')]" />
                    
                </xpath>
           </field>
      </record>

