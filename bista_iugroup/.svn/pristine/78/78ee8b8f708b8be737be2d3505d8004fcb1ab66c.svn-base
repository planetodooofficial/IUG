<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!--Overiding Menues -->
        <record id="sales_team.menu_base_partner" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('sales_team.group_sale_salesman'))]"/>
            <field name="name">Sales</field>
        </record>
        <record id="sales_team.menu_sales" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('sales_team.group_sale_salesman'))]"/>
            <field name="name">Sales</field>
        </record>

        <record id="hr_timesheet.timesheet_menu_root" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('base.group_user'))]"/>
        </record>

        <record id="stock.menu_stock_root" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('stock.group_stock_user'))]"/>
            <field name="name">Warehouse</field>
            <field name="sequence" eval="70"/>
        </record>
        <record id="account.menu_finance" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('account.group_account_invoice'))]"/>
            <field name="name">Invoicing</field>
            <field name="sequence" eval="50"/>
        </record>
        <record id="mail.mail_feeds_main" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('base.group_user')),(4, ref('hr.group_hr_manager')),(4, ref('hr.group_hr_user'))]"/>
            <field name="name">Messaging</field>
            <field name="sequence" eval="5"/>
        </record>
        <record id="base.menu_reporting" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('base.group_user')),(4, ref('hr.group_hr_manager')),(4, ref('hr.group_hr_user'))]"/>
            <field name="name">Reporting</field>
            <field name="sequence" eval="170"/>
        </record>
        <record id="hr.menu_hr_root" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('base.group_user'))]"/>
            <field name="name">Human Resources</field>
            <field name="sequence" eval="90"/>
        </record>
        <record id="base.menu_administration" model="ir.ui.menu">
            <field name="groups_id" eval="[(3,ref('base.group_erp_manager'))]"/>
            <field name="name">Settings</field>
            <field name="sequence" eval="500"/>
        </record>



        <!-- +++++++++ Inheriting Partner Titles ++++++++++++++ -->
        <record id="view_partner_title_tree_inherit_iu" model="ir.ui.view">
            <field name="name">res.partner.title.tree</field>
            <field name="model">res.partner.title</field>
            <field eval="8" name="priority"/>
            <field name="inherit_id" ref="base.view_partner_title_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='shortcut']" position="after">
                    <field name="company_id" groups="base.group_multi_company"/>
                </xpath>
            </field>
        </record>

        <record id="view_partner_title_form_inherit_iu" model="ir.ui.view">
            <field name="name">res.partner.title.form</field>
            <field name="model">res.partner.title</field>
            <field eval="8" name="priority"/>
            <field name="inherit_id" ref="base.view_partner_title_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='shortcut']" position="after">
                    <field name="company_id" groups="base.group_multi_company" widget="selection" required="1"/>
                </xpath>
            </field>
        </record>
        <!--  +++++ ADDING  Fileds in PArent Partner View +++++ -->
        <record id="view_res_partner_filter_inherit_iu1" model="ir.ui.view">
            <field name="name">res.partner.select</field>
            <field name="model">res.partner</field>
            <field eval="8" name="priority"/>
            <field name="inherit_id" ref="base.view_res_partner_filter"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="replace">
                    <field name="complete_name"/>
                    <field name="name" string="First Name" filter_domain="[('name','ilike',self)]"/>
                    <field name="last_name"/>
                    <field name="middle_name"/>
                </xpath>
                <xpath expr="/search/filter[@name='supplier']" position="after">
                    <filter string="Active" name="active" domain="[('active','=',1)]" help="Active Partners"/>
                    <filter string="InActive" name="inactive" domain="[('active','=',0)]" help="InActive Partners"/>
                </xpath>
            </field>
        </record>

        <record id="view_partner_tree_inherit_iu1" model="ir.ui.view">
            <field name="name">res.partner.tree</field>
            <field name="model">res.partner</field>
            <field eval="50" name="priority"/>
            <field name="inherit_id" ref="base.view_partner_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='display_name']" position="replace">
                    <field name="name" string="First Name"/>
                    <field name="middle_name" attrs="{'invisible': [('cust_type','=','customer')]}"/>
                    <field name="last_name" attrs="{'invisible': [('cust_type','=', 'customer')]}"/>
                    <field name="complete_name" invisible="1"/>
                    <field name="cust_type" invisible="1"/>
                </xpath>
                <xpath expr="//field[@name='email']" position="after">
                    <field name="street" invisible="1"/>
                    <field name="fax" />
                    <field name="city" />
                    <field name="zip" />
                    <field name="zone_id"/>
                    <field name="meta_zone_id" invisible="1"/>
                </xpath>
            </field>
        </record>

        <record id="view_partner_tree_inherit_customer" model="ir.ui.view">
            <field name="name">res.partner.tree</field>
            <field name="model">res.partner</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Contacts">
                    <field name="name" string="Company Name"/>
                    <field name="middle_name" invisible="1"/>
                    <field name="last_name" invisible="1"/>
                    <field name="complete_name" invisible="1"/>
                    <field name="fax" />
                    <field name="function" invisible="1"/>
                    <field name="phone"/>
                    <field name="email" />
                    <field name="street" />
                    <field name="city" />
                    <field name="zip" />
                    <field name="zone_id"/>
                    <field name="sales_representative_id"/>
                    <field name="company_id" groups="base.group_multi_company" widget="selection"/>
                    <field name="meta_zone_id" invisible="1"/>
                    <field name="ref" invisible="1"/>
                    <field name="cust_type" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                    <field name="is_company" invisible="1"/>
                    <field name="country_id" invisible="1"/>
                    <field name="parent_id" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="view_partner_tree_inherit_contact" model="ir.ui.view">
            <field name="name">res.partner.tree</field>
            <field name="model">res.partner</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Contacts">
                    <field name="name" string="First Name"/>
                    <field name="middle_name" invisible = "1"/>
                    <field name="last_name" attrs="{'invisible': [('cust_type','=', 'customer')]}"/>
                    <field name="complete_name" invisible="1"/>
                    <field name="fax" />
                    <field name="function" invisible="1"/>
                    <field name="phone"/>
                    <field name="email" />
                    <field name="parent_id" string="Related Company"/>
                    <field name="street" invisible="1"/>
                    <field name="city" />
                    <field name="zip" />
                    <field name="zone_id"/>
                    <field name="company_id" groups="base.group_multi_company" widget="selection"/>
                    <field name="meta_zone_id" invisible="1"/>
                    <field name="cust_type" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                    <field name="is_company" invisible="1"/>
                    <field name="country_id" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="view_simplified_form" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">res.partner</field>
            <field eval="100" name="priority"/>
            <field name="arch" type="xml">
                <form string="contacts" version="7.0" delete="false">
                    <header>
                    </header>
                    <sheet>
                        <notebook colspan="4">
                            <page string="General Info">
                                <group colspan="4" col="6" >
                                    <group>
                                        <field name="name" readonly="1" string="Name" default_focus="1" placeholder="First Name" colspan="1" widget="linkedin"/>
                                    </group>
                                    <group >
                                        <field name="middle_name" readonly="1" placeholder="Middle Name" colspan="1" widget="linkedin" />
                                    </group>
                                    <group>
                                        <field name="last_name" readonly="1" placeholder="Last Name" colspan="1" widget="linkedin" />
                                    </group>
                                </group>
                                <group invisible="1">
                                    <field name="customer" />
                                    <field name="supplier" />
                                    <field name="active" />
                                    <field name="is_company" />
                                    <field name="cust_type" />
                                    <field name="company_id" />
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_contact_form" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">res.partner</field>
            <field eval="50" name="priority"/>
            <field name="arch" type="xml">
                <form string="contacts"  delete="false">
                    <header>
                        <button string="Upload Attachment" style="margin:10px" name="upload_attachment" type="object" class="oe_highlight"
                                groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button name="create_customer_login" style="margin:10px" string="Create Customer Login" type="object" class="oe_highlight" attrs="{'invisible': [('has_login','=',True)]}"
                                groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_recruiter,bista_iugroup.group_iu_cust_service" context="{'cust_type': cust_type}"/>
                        <button string="Resend Login Info" style="margin:10px" name="resend_login_mail" type="object" class="oe_highlight" attrs="{'invisible': [('has_login','!=',True)]}"
                                groups="bista_iugroup.group_iu_cust_service,bista_iugroup.group_record_manager,bista_iugroup.group_iu_recruiter"/>
                    </header>
                    <sheet>
                        <field invisible="1" name="image" widget='image' class="oe_left oe_avatar" options='{"preview_image": "image_medium", "size": [90, 90]}'/>
                        <newline/>
                        <div class="oe_title" >
                            <div class="oe_edit_only">
                                <label for="ref" string="Reference"/>
                            </div>
                            <h1>
                                <field name="has_login" invisible="1"/>
                                <field name="ref"  placeholder="Reference" string="Reference"/>
                            </h1>
                        </div>
                        <div class="oe_inline">
                            <button name="view_past_appointments_contact"  style="margin:10px" string="View Past Events" type="object" class="oe_highlight"
                                    groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                            <button name="view_today_events_for_contact"  style="margin:10px" string="View Todays Events" type="object" class="oe_highlight"
                                    groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                            <button name="view_future_events_for_contact"  style="margin:10px" string="View Future Events" type="object" class="oe_highlight"
                                    groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        </div>
                        <notebook colspan="4">
                            <page string="General Info">
                                <separator string="Personal Information" />
                                <group>
                                    <group>
                                        <field name="call_date" />
                                        <field name="active"/>
                                        <field name="billing_contact" />
                                        <field name="gender"/>
                                        <field name="suppress_email"/>
                                        <field name="contract_no"/>
                                    </group>
                                    <group>
                                        <field name="cust_type" readonly="1"/>
                                        <field name="ordering_contact" />
                                        <field name="company_id" groups="base.group_multi_company" widget="selection" required="1"/>
                                        <field name="title" domain="[('company_id','=',company_id)]"/>
                                        <field name="user_id" domain="[('company_id','=',company_id)]" options="{'limit': 7, 'create': false, 'create_edit': false, 'no_open': True}"
                                               string="Related User" readonly="1" context="{'show_login': 1}"/>
                                    </group>
                                </group>
                                <div>
                                    <label string="Name :- "/>
                                    <newline/>
                                        <field name="name"  style="padding-right:10px" string="Name" default_focus="1" class="oe_inline" placeholder="First Name" widget="linkedin"/>
                                        <field name="middle_name"  style="padding-right:10px" placeholder="Middle Name" class="oe_inline"  widget="linkedin" />
                                        <field name="last_name"  style="padding-right:10px" placeholder="Last Name" widget="linkedin" class="oe_inline" />
                                </div>
                                <separator string="Company Information" />
                                <group>
                                    <group>
                                        <field name="parent_id" placeholder="Company" domain="[('customer', '=', 1),('cust_type', '=', 'customer'),
                                                            ('company_id','=',company_id)]"
                                               context="{'default_customer': 'True','form_view_ref':'bista_iugroup.view_partner_form_inherit_iu'}"
                                               options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="department" placeholder="e.g. Sales"  />
                                    </group>
                                    <group>
                                        <field name="function" placeholder="e.g. Sales Director"/>
                                        <field name="sales_representative_id"/>
                                    </group>
                                </group>
                                <separator string="Feedback emails" />
                                <group>
                                    <field name="opt_out_of_feedback_emails"/>
                                </group>
                                <separator string="Contact Information"/>
                                <group>
                                    <group>
                                        <field name="fax"/>
                                        <field name="zone_id"  domain="[('company_id','=',company_id)]" />
                                        <field name="meta_zone_id" domain="[('company_id','=',company_id)]"/>
                                    </group>
                                    <group>
                                        <label for="phone" string="Phone-1"/>
                                        <div>
                                            <field name="ext_phone1" class="oe_inline" placeholder="Ext" style="max-width: 15%%;"/>
                                            <field name="phone" class="oe_inline" placeholder="e.g. 328-181-3700" required="1"/>
                                            <field name="phone_type_id1" class="oe_inline" domain="[('company_id','=',company_id)]" style="max-width: 35%%;"/>
                                        </div>
                                        <label for="phone2" string="Phone-2"/>
                                        <div>
                                            <field name="ext_phone2" class="oe_inline" placeholder="Ext" style="max-width: 15%%;"/>
                                            <field name="phone2" class="oe_inline" placeholder="e.g. 328-181-3700"/>
                                            <field name="phone_type_id2" class="oe_inline" domain="[('company_id','=',company_id)]" style="max-width: 35%%;"/>
                                        </div>
                                    </group>
                                </group>
                                <separator string="Contact Note"/>
                                <field name="comment" placeholder="Put an internal note..." nolabel="1" colspan="4" widget="html"/>
                                <field name="customer" context="{'default_customer': 1}" invisible="1"/>
                                <field name="supplier" context="{'default_supplier': 0}" invisible="1"/>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers" invisible="1"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>





        <record id="view_partner_form_inherit_iu" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">res.partner</field>
            <field eval="40" name="priority"/>
            <field name="arch" type="xml">
                <form string="Partners"  delete="false">
                    <header>
                        <button string="Upload Attachment" name="upload_attachment" type="object" class="oe_highlight"
                                groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                    </header>
                    <sheet>
                        <field invisible="1" name="image" widget='image' class="oe_left oe_avatar" options='{"preview_image": "image_medium", "size": [90, 90]}'/>
                        <newline/>
                        <div class="oe_title">
                            <div class="oe_edit_only">
                                <label for="ref" string="Reference"/>
                            </div>
                            <h1>
                                <field name="ref"  placeholder="Reference" string="Reference" readonly="1"/>
                            </h1>
                            <button name="create_event" style="margin:10px" string="Schedule Event" type="object" class="oe_highlight"
                                    groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                            <button name="view_past_appointments_customer" style="margin:10px" string="View Past Events" type="object" class="oe_highlight"
                                    groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                            <button name="view_today_events_for_customer" style="margin:10px" string="View Todays Events" type="object" class="oe_highlight"
                                    groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                            <button name="view_future_events_for_customer" style="margin:10px" string="View Future Events" type="object" class="oe_highlight"
                                    groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        </div>
                        <notebook colspan="4">
                            <page string="General">
                                <separator string="Personal Information" />
                                <h3>
                                    <group>
                                        <group>
                                            <field name="name" string="Name/Company" default_focus="1" placeholder="First Name / Company" colspan="1" widget="linkedin"/>
                                        </group>
                                        <group>
                                            <field name="last_name" string="Last Name" placeholder="Last Name" colspan="1" widget="linkedin"/>
                                        </group>
                                    </group>
                                </h3>
                                <group>
                                    <group>
                                        <field name="company_name"  placeholder="Company Name"  />
                                        <field name="parent_cust_id" domain="[('cust_type','=','customer'),('company_id','=',company_id)]"
                                               options="{'limit': 7, 'create': false, 'create_edit': false, 'no_open': True}"/>
                                    </group>
                                    <group>
                                        <field name="short_name"  placeholder="Short Name"  />
                                    </group>
                                </group>
                                <group>
                                    <group>
                                        <field name="street" style="width:100%" placeholder="Street 1..." />
                                        <field name="street2" style="width:100%"  placeholder="Street 2..."/>
                                        <field name="city" style="width:100%" placeholder="City" required="1"/>
                                        <field name="company_id" style="width:100%" groups="base.group_multi_company" widget="selection" required="1"/>
                                        <field name="cust_type" style="width:100%" />
                                        <field name="sales_representative_id" style="width:100%"/>
                                        <field name="active"/>
                                        <field name="fee_note"/>
                                        <field name="order_note" />
                                        <field name="customer_basis" />
                                        <field name="need_glcode" string="Need GLCode?"/>
                                        <field name="has_login" invisible="1"/>
                                        <field name="customer_group_id" widget='selection'/>
                                        <button name="open_map_new" string="Map" type="object" style="width:40%" icon="fa-search-plus"/>
                                    </group>
                                    <group>
                                        <field name="state_id" class="oe_no_button" placeholder="State"
                                               required="1" options="{'limit': 7, 'create': false, 'create_edit': false ,'no_open': True}" />
                                        <field name="country_id" placeholder="Country" class="oe_no_button"
                                               options="{'limit': 7, 'create': false, 'create_edit': false ,'no_open': True}" />
                                        <field name="zip" placeholder="ZIP"  required="1"/>
                                        <field name="zone_id"  domain="[('company_id','=',company_id)]" />
                                        <field name="meta_zone_id" domain="[('company_id','=',company_id)]"/>
                                        <field name="user_id" domain="[('company_id','=',company_id)]" options="{'limit': 7, 'create': false, 'create_edit': false, 'no_open': True}"
                                               string="Related User" context="{ 'show_login': 1}"/>
                                        <field name="scheduler_id" attrs="{'required': [('customer_basis','=', True)]}"/>
                                        <field name="mental_prog"/>
                                        <field name="suppress_email"/>
                                        <field name="is_monthly" />
                                        <field name="telephone_interpretation" />
                                    </group>
                                </group>
                                <group>
                                    <group>
                                        <field name="gpuid" attrs="{'required': [('need_glcode','=', True)]}"/>
                                    </group>
                                    <group>
                                        <field name="csid" attrs="{'required': [('need_glcode','=', True)]}"/>
                                    </group>
                                </group>
                                <separator string="Project Details" invisible="1"/>
                                <group>
                                    <field name="project_part_ids" nolabel="1" invisible="1">
                                        <tree>
                                            <field name="name"/>
                                        </tree>
                                    </field>
                                </group>
                                <separator string="Contact Information"/>
                                <group>
                                    <group>
                                        <label for="phone" string="Phone 1"/>
                                        <field name="ext_phone1" nolabel="1" style="width:168px" placeholder="Ext"/>
                                        <separator/>
                                        <field name="phone" cols="2" nolabel="1" style="width:168px" placeholder="e.g. 328-181-3700"/>
                                        <label for="phone2" string="Phone 2"/>
                                        <field name="ext_phone2" nolabel="1" style="width:168px" placeholder="Ext"/>
                                        <separator/>
                                        <field name="phone2" nolabel="1" style="width:168px" placeholder="e.g. 328-181-3700"/>
                                    </group>
                                    <group >
                                        <field name="email" widget="email" placeholder="Email 1.." string="Email 1" />
                                        <field name="email2" widget="email" placeholder="Email 2.." />
                                        <field name="website" widget="url" placeholder="e.g. www.openerp.com"/>
                                        <field name="fax"/>
                                    </group>
                                </group>
                                <separator string="Contact Note" />
                                <field name="comment" placeholder="Put an internal note..." nolabel="1" colspan="4" widget="html"/>
                                <group invisible="1">
                                    <group>
                                        <field name="customer" />
                                    </group>
                                </group>
                                <separator string="Contacts"/>
                                <field name="child_ids" mode="kanban" context="{'cust_type': 'contact', 'default_customer':1,'default_supplier':0,'default_is_company': 0,
                                                'default_company_id': company_id,'form_view_ref':'bista_iugroup.view_contact_form'}">
                                    <kanban create="false" delete="false">
                                        <field name="color"/>
                                        <field name="cust_type"/>
                                        <field name="name"/>
                                        <field name="last_name"/>
                                        <field name="ref"/>
                                        <field name="title"/>
                                        <field name="email"/>
                                        <field name="parent_id" />
                                        <field name="is_company"/>
                                        <field name="function"/>
                                        <field name="phone"/>
                                        <field name="fax"/>
                                        <!--<field name="has_image"/>-->
                                        <templates>
                                            <t t-name="kanban-box">
                                                <t t-set="color" t-value="kanban_color(record.color.raw_value)"/>
                                                <div t-att-class="color + (record.title.raw_value == 1 ? ' oe_kanban_color_alert' : '')" style="position: relative">
                                                    <a t-if="! read_only_mode" type="delete" style="position: absolute; right: 0; padding: 4px; diplay: inline-block">X</a>
                                                    <div class="oe_module_vignette">
                                                        <a type="open">
                                                            <t t-if="record.image and record.image.raw_value">
                                                                <img t-att-src="kanban_image('res.partner', 'image', record.id.value, {'preview_image': 'image_small'})"
                                                                     class="oe_avatar oe_kanban_avatar_smallbox"/>
                                                            </t>
                                                            <t t-if="record.image and record.image.raw_value !== false">
                                                                <img t-att-src="'data:image/png;base64,'+record.image.raw_value" class="oe_avatar oe_kanban_avatar_smallbox"/>
                                                            </t>
                                                            <t t-if="!record.image or record.image.raw_value === false">
                                                                <t t-if="record.is_company.raw_value === true">
                                                                    <img t-att-src='_s + "/base/static/src/img/company_image.png"' class="oe_kanban_image oe_kanban_avatar_smallbox"/>
                                                                </t>
                                                                <t t-if="record.is_company.raw_value === false">
                                                                    <img t-att-src='_s + "/base/static/src/img/avatar.png"' class="oe_kanban_image oe_kanban_avatar_smallbox"/>
                                                                </t>
                                                            </t>
                                                        </a>
                                                        <div class="oe_module_desc">
                                                            <div class="oe_kanban_box_content oe_kanban_color_bglight oe_kanban_color_border">
                                                                <table class="oe_kanban_table">
                                                                    <tr>
                                                                        <td class="oe_kanban_title1" align="left" valign="middle">
                                                                            <h4><a type="open">[<field name="ref"/>] <field name="name"/> <field name="middle_name"/> <field name="last_name"/></a></h4>
                                                                            <i><div t-if="record.function.raw_value">
                                                                                <field name="function"/></div></i>
                                                                            <div>
                                                                                <a t-if="record.email.raw_value" title="Mail" t-att-href="'mailto:'+record.email.value">
                                                                                    <field name="email"/>
                                                                                </a>
                                                                            </div>
                                                                            <div t-if="record.phone.raw_value">Phone: <field name="phone"/></div>
                                                                            <div t-if="record.mobile.raw_value">Mobile: <field name="mobile"/></div>
                                                                            <div t-if="record.fax.raw_value">Fax: <field name="fax"/></div>
                                                                        </td>
                                                                    </tr>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </t>
                                        </templates>
                                    </kanban>
                                </field>
                            </page>
                            <page string="Other" colspan="4">
                                <group>
                                    <group >
                                        <group>
                                            <field name="ssnid" style="width: 190px;"/>
                                            <field name="sinid" style="width: 190px;"/>
                                            <field name="vat" string="TIN No" style="width: 190px;"/>
                                            <field name="special_customer"/>
                                        </group>
                                    </group>
                                    <group>
                                        <field name="billing_partner_id" domain="[('customer', '=', 1),('cust_type', '=','customer'),('is_company', '=',0),('company_id','=',company_id)]"
                                               context="{ 'form_view_ref':'bista_iugroup.view_partner_form_inherit_iu'}"
                                               options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="head_contact_id" domain="[('customer', '=', 1),('cust_type', '=', 'customer'),('is_company', '=',0),('company_id','=',company_id)]"
                                               context="{ 'form_view_ref':'bista_iugroup.view_contact_form'}" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="billing_contact_id" domain="[('customer', '=', 1),('cust_type', '=', 'customer'),('is_company', '=',0),('company_id','=',company_id)]"
                                               context="{ 'form_view_ref':'bista_iugroup.view_contact_form'}"
                                               options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="interpreter_id" domain="[('supplier', '=', 1),('cust_type', '=', 'interpreter'),('is_company', '=',0),('company_id','=',company_id)]"
                                               context="{ 'form_view_ref':'bista_iugroup.view_interpreter_form'}"
                                               options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="property_payment_term_id" widget="selection"/>
                                    </group>
                                </group>
                                <group>
                                    <h3 style="color: #5F9EA0;"> Related Locations: </h3>
                                </group>
                                <field name="location_lines" nolabel="1" context="{'company_id': company_id,'form_view_ref':'bista_iugroup.view_location_form'}" domain="[('company_id','=', company_id)]">
                                    <tree >
                                        <field name="name" />
                                        <field name="phone" />
                                        <field name="street" />
                                        <field name="street2" />
                                        <field name="city" />
                                        <field name="state_id" />
                                        <field name="country_id" />
                                    </tree>
                                </field>
                                <group>
                                    <h3 style="color: #5F9EA0;"> Restricted Interpreters List: </h3>
                                </group>
                                <group>
                                    <field name="block_inter_ids" nolabel="1" domain="[('company_id','=', company_id),('cust_type','in',['interpreter','interp_and_transl']),('supplier','=',True)]"/>
                                </group>
                            </page>
                            <page string="Billing" colspan="4" groups="account.group_account_user,account.group_account_manager,bista_iugroup.group_iu_cust_service">
                                <group>
                                    <group>
                                        <field name="rate" string="Per Mile Bill Rate"/>
                                        <field name="contract_no" />
                                    </group>
                                    <group>
                                        <field name="bill_miles_after" />
                                        <field name="discount" />
                                    </group>
                                    <group>
                                    </group>
                                </group>
                                <group colspan="4" col="6">
                                    <separator string="Language Rate"/>
                                    <newline/>
                                    <field name="rate_ids" options="{'reload_on_button': true}" colspan="2" nolabel="1">
                                        <tree  string="Rates" editable="bottom">
                                            <field name="rate_type" />
                                            <field name="base_hour" />
                                            <field name="inc_min" />
                                            <field name="default_rate" />
                                            <field name="spanish_regular" />
                                            <field name="spanish_licenced" />
                                            <field name="spanish_certified" />
                                            <field name="exotic_regular" />
                                            <field name="exotic_certified" />
                                            <field name="exotic_middle" />
                                            <field name="exotic_high" />
                                        </tree>
                                    </field>
                                    <newline/>
                                    <separator string="Transporter Rate"/>
                                    <newline/>
                                    <field name="transporter_rate_lines" nolabel="1">
                                        <tree string="Transporter Rate" editable="top">
                                            <field name="date" invisible="1"/>
                                            <field name="type" />
                                            <field name="rate" />
                                            <field name="equiv_min_miles" />
                                            <field name="min_round_trip" />
                                            <field name="no_show" />
                                            <field name="wait_time" />
                                            <field name="pickup_fee" />
                                            <field name="gratuity" />
                                            <field name="cancel_fee" />
                                            <field name="after_hours" />
                                            <field name="comment" />
                                        </tree>
                                    </field>
                                </group>
                                <separator string="Billing Note"/>
                                <field name="billing_comment" string="Billing Note" placeholder="Put an Billing Note..." nolabel="1" colspan="4" widget="html"/>
                                <separator string="Rubrik"/>
                                <field name="rubrik" string="Rubrik" placeholder="Rubrik..." nolabel="1" colspan="4"/>
                            </page>
                            <page string="History" invisible="1">
                                <separator string="Interpreter History"/>
                                <field name="interpreter_history" readonly="1" domain="[('state','not in',('voicemailsent'))]" invisible="1">
                                    <tree>
                                        <field name="name" context="{ 'form_view_ref':'bista_iugroup.view_interpreter_form'}"/>
                                        <field name="language_id" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="city" />
                                        <field name="rate" />
                                        <field name="event_date" />
                                        <field name="state" />
                                    </tree>
                                </field>
                            </page>
                            <!--<page string="Authorise Details" groups="account.group_account_user,account.group_account_manager">-->
                            <!--<field name="customer_profile_id" readonly="True"/>-->
                            <!--<group colspan="2" col="4">-->
                            <!--<group>-->
                            <!--<field name="auth_cc_number" password="True"/>-->
                            <!--<button name="customer_profile" string="Validate Credit Card" type="object" class="oe_highlight"-->
                            <!--groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>-->
                            <!--</group>-->
                            <!--<group>-->
                            <!--<field name="auth_cc_expiration_date"/>-->
                            <!--</group>-->
                            <!--</group>-->
                            <!--<newline/>-->
                            <!--&lt;!&ndash;<field name="profile_ids" nolabel="1" colspan="4" readonly="True"/>&ndash;&gt;-->
                            <!--<field name="credit_card_lines" nolabel="1" />-->
                            <!--</page>-->
                            <page string="Accounting" col="4" name="accounting" groups="account.group_account_user,account.group_account_manager">
                                <group>
                                    <group>
                                        <field name="property_account_position_id" widget="selection"/>
                                    </group>
                                    <group>
                                        <field name="last_time_entries_checked"/>
                                    </group>
                                    <group>
                                        <field name="property_account_receivable_id" groups="account.group_account_invoice"
                                               options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="credit"/>
                                        <field name="credit_limit"/>
                                    </group>
                                    <group>
                                        <field name="property_account_payable_id" groups="account.group_account_invoice"
                                               options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}"/>
                                        <field name="debit"/>
                                    </group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"  invisible="1"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

        <!-- Fields of Billing Page readonly for customer-service group  -->
        <record model="ir.ui.view" id="view_partner_form_inherit_iu_readonly_customer_service">
            <field name="name">view.partner.form.customer.service</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="bista_iugroup.view_partner_form_inherit_iu" />
            <field name="groups_id" eval="[(6, 0, [ref('bista_iugroup.group_iu_cust_service') ])]" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='rate']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='bill_miles_after']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='discount']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='contract_no']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='rate_ids']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='transporter_rate_lines']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='billing_comment']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='rubrik']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
            </field>
        </record>

        <record id="view_res_partner_filter_inherit_iu" model="ir.ui.view">
            <field name="name">res.partner.select</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <search string="Search Partner">
                    <field name="complete_name"/>
                    <field name="name" string="First Name"/>
                    <field name="last_name"/>
                    <field name="middle_name"/>
                    <field name="ref"/>
                    <field name="phone"/>
                    <field name="email"/>
                    <field name="fax"/>
                    <field name="street" />
                    <field name="city"/>
                    <field name="zip"/>
                    <field name="zone_id"/>
                    <field name="meta_zone_id" />
                    <field name="country_id"/>
                    <filter help="My Contacts" icon="fa-user" domain="[('user_id','=',uid)]"/>
                    <separator/>
                    <filter string="Persons" name="type_person" domain="[('is_company','=',0)]"/>
                    <filter string="Companies" name="type_company" domain="[('is_company','=',1)]"/>
                    <filter string="Active" name="active" domain="[('active','=',1)]" help="Active Partners"/>
                    <filter string="InActive" name="inactive" domain="[('active','=',0)]" help="InActive Partners"/>
                    <separator/>
                    <field name="user_id"/>
                    <field name="parent_id" domain="[('is_company','=',1)]" filter_domain="[('parent_id','child_of',[self])]"/>
                    <group expand="0" string="Group By...">
                        <filter string="Salesperson" icon="fa-user" domain="[]" context="{'group_by' : 'user_id'}" />
                        <filter string="Company" context="{'group_by': 'parent_id'}"/>
                        <filter string="country_id" context="{'group_by': 'country_id'}"/>
                        <filter string="Companies" icon="fa-user" domain="[]" context="{'group_by' : 'company_id'}" />
                        <filter string="Zone" context="{'group_by': 'zone_id'}"/>
                        <filter string="Meta Zone" context="{'group_by': 'meta_zone_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <!-- Contact Kanban View -->
        <record model="ir.ui.view" id="res_partner_kanban_view_inherit">
            <field name="name">res.partner.kanban</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <kanban>
                    <field name="color"/>
                    <field name="name"/>
                    <field name="middle_name"/>
                    <field name="last_name"/>
                    <field name="title"/>
                    <field name="email"/>
                    <field name="parent_id"/>
                    <field name="is_company"/>
                    <field name="function"/>
                    <field name="phone"/>
                    <field name="street"/>
                    <field name="street2"/>
                    <field name="zip"/>
                    <field name="city"/>
                    <field name="country_id"/>
                    <field name="mobile"/>
                    <field name="state_id"/>
                    <field name="image"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_vignette oe_semantic_html_override">
                                <a type="open">
                                    <t t-if="record.image and record.image.raw_value">
                                        <img t-att-src="kanban_image('contact', 'image_small', record.id.value)" class="oe_kanban_image"/>
                                    </t>
                                    <t t-if="!record.image and record.image.raw_value">
                                        <t t-if="record.is_company.raw_value === true">
                                            <img t-att-src='_s + "/base/static/src/img/company_image.png"' class="oe_kanban_image"/>
                                        </t>
                                        <t t-if="record.is_company.raw_value === false">
                                            <img t-att-src='_s + "/base/static/src/img/avatar.png"' class="oe_kanban_image"/>
                                        </t>
                                    </t>
                                </a>
                                <div class="oe_kanban_details">
                                    <h4 class="oe_partcontactding"><a type="open">[<field name="ref"/>] <field name="name"/> <field name="middle_name"/> <field name="last_name"/></a></h4>
                                    <div class="oe_kanban_partner_categories"/>
                                    <div class="oe_kanban_partner_links"/>
                                    <ul>
                                        <li t-if="record.parent_id.raw_value and !record.function.raw_value"><field name="parent_id"/></li>
                                        <li t-if="!record.parent_id.raw_value and record.function.raw_value"><field name="function"/></li>
                                        <li t-if="record.parent_id.raw_value and record.function.raw_value"><field name="function"/> at <field name="parent_id"/></li>
                                        <li t-if="record.city.raw_value and !record.country_id.raw_value"><field name="city"/></li>
                                        <li t-if="!record.city.raw_value and record.country_id.raw_value"><field name="country_id"/></li>
                                        <li t-if="record.city.raw_value and record.country_id.raw_value"><field name="city"/>, <field name="country_id"/></li>
                                        <li t-if="record.email.raw_value"><a t-attf-href="mailto:#{record.email.raw_value}"><field name="email"/></a></li>
                                        <li t-if="record.phone.raw_value"><field name="phone"/></li>
                                    </ul>
                                </div>
                            </div>

                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <record id="action_partner_form_inherit_iu" model="ir.actions.act_window">
            <field name="name">Customers</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,kanban</field>
            <field name="context">{"default_customer":1,"default_supplier":0,"default_is_company":0,"default_cust_type":'customer'}</field>
            <field name="domain">[('cust_type','=','customer'),('customer','=',1)]</field>
            <field name="search_view_id" ref="view_res_partner_filter_inherit_iu"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to add a contact in your address book.
                </p><p>
                OpenERP helps you easily track all activities related to
                a customer; discussions, history of business opportunities,
                documents, etc.
            </p>
            </field>
        </record>

        <record id="action_partner_form_view1_inherit_iu" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">kanban</field>
            <field name="view_id" ref="res_partner_kanban_view_inherit"/>
            <field name="act_window_id" ref="action_partner_form_inherit_iu"/>
        </record>

        <record id="action_partner_form_view2_inherit_iu" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_partner_form_inherit_iu"/>
            <field name="act_window_id" ref="action_partner_form_inherit_iu"/>
        </record>

        <record id="action_partner_tree_inherit_iu_customer1" model="ir.actions.act_window.view">
            <field name="sequence" eval="0"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_partner_tree_inherit_customer"/>
            <field name="act_window_id" ref="action_partner_form_inherit_iu"/>
        </record>

        <menuitem id="menu_customer_iu" parent="menu_contacts" action="action_partner_form_inherit_iu" sequence="1"
                  groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter" />

        <record id="action_partner_form_inherit_iu1" model="ir.actions.act_window">
            <field name="name">Contacts</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,kanban</field>
            <field name="context">{"search_default_cust_type":'contact',"default_cust_type":'contact',"default_is_company":0,
                "default_customer":1,"default_supplier":0}</field>
            <field name="domain">[('cust_type','=','contact'),('customer','=',1)]</field>
            <field name="search_view_id" ref="view_res_partner_filter_inherit_iu"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to add a contact in your address book.
                </p><p>
                OpenERP helps you easily track all activities related to
                a customer; discussions, history of business opportunities,
                documents, etc.
            </p>
            </field>
        </record>

        <record id="action_partner_form_view1_inherit_iu1" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">kanban</field>
            <field name="view_id" ref="res_partner_kanban_view_inherit"/>
            <field name="act_window_id" ref="action_partner_form_inherit_iu1"/>
        </record>

        <record id="action_partner_form_view2_inherit_iu1" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_contact_form"/>
            <field name="act_window_id" ref="action_partner_form_inherit_iu1"/>
        </record>

        <record id="action_partner_tree_inherit_contact1" model="ir.actions.act_window.view">
            <field name="sequence" eval="0"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_partner_tree_inherit_contact"/>
            <field name="act_window_id" ref="action_partner_form_inherit_iu1"/>
        </record>

        <menuitem id="menu_customer_iu1" parent="menu_contacts" action="action_partner_form_inherit_iu1" sequence="1"
                  groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
    </data>
</odoo>
