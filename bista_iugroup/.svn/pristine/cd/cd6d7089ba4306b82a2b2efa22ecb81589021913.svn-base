<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Events -->
        <record id="view_event_tree" model="ir.ui.view">
            <field name="name">event.tree</field>
            <field name="model">event</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Events" decoration-bf="created_by in ('customer','contact')"
                      decoration-primary="state == 'draft'"  decoration-info="state=='confirmed'"
                      decoration-muted="state=='unbilled'" decoration-success="state == 'scheduled'"
                      decoration-danger="state == 'cancel'">
                    <field name="name" string="Event Id"/>
                    <field name="partner_id" string="Billing Customer"/>
                    <field name="ordering_partner_id" string="Ordering Customer" invisible="1"/>
                    <field name="language_id" />
                    <field name="event_start" />
                    <field name="event_end" />
                    <field name="event_start_date" invisible="1"/>
                    <field name="event_start_time" invisible="1"/>
                    <field name="event_end_time" invisible="1"/>
                    <field name="single_interpreter"/>
                    <field name="all_interpreter_email" string="Interpreter Email"/>
                    <field name="interpreters_phone" string="Interpreter Phone"/>
                    <field name="patient_id" string="Claimant"/>
                    <field name="location_id" />
                    <field name="cancel_reason_id" invisible="1"/>
                    <field name="created_by" invisible="1"/>
                    <field name="company_id" groups="base.group_multi_company"/>
                    <field name="state" />
                    <field name="event_type" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="view_translation_event_tree" model="ir.ui.view">
            <field name="name">translation.event.tree</field>
            <field name="model">event</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Events" decoration-bf="created_by in ('customer','contact')"
                      decoration-primary="state == 'draft'"  decoration-info="state=='confirmed'"
                      decoration-muted="state=='unbilled'" decoration-success="state == 'scheduled'"
                      decoration-danger="state == 'cancel'">
                    <field name="name" string="Event Id"/>
                    <field name="partner_id" string="Billing Customer"/>
                    <field name="ordering_partner_id" string="Ordering Customer" invisible="1"/>
                    <field name="language_id" />
                    <field name="event_start" />
                    <field name="event_end" />
                    <field name="event_start_date" invisible="1"/>
                    <field name="event_start_time" invisible="1"/>
                    <field name="event_end_time" invisible="1"/>
                    <field name="project_name_id" string="Project"/>
                    <field name="ref" string="Reference"/>
                    <field name="location_id" />
                    <field name="cancel_reason_id" invisible="1"/>
                    <field name="created_by" invisible="1"/>
                    <field name="company_id" groups="base.group_multi_company"/>
                    <field name="state" />
                    <field name="event_type" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="view_event_unbilled_tree" model="ir.ui.view">
            <field name="name">event.tree</field>
            <field name="model">event</field>
            <field eval="9" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Events" decoration-bf="created_by in ('customer','contact')"
                      decoration-muted="state=='unbilled'" decoration-info="state=='confirmed'"
                      decoration-success="state in ('scheduled')" decoration-danger="state in ('cancel')">
                    <field name="name" string="Event Id"/>
                    <field name="partner_id" string="Billing Customer"/>
                    <field name="ordering_partner_id" string="Ordering Customer" invisible="1"/>
                    <field name="language_id" />
                    <field name="project_name_id" string="Project"/>
                    <field name="ref" string="Ref"/>
                    <field name="event_start" />
                    <field name="event_end" />
                    <field name="event_start_date" invisible="1"/>
                    <field name="event_start_time" invisible="1"/>
                    <field name="event_end_time" invisible="1"/>
                    <field name="schedule_event_time" invisible="1" />
                    <field name="single_interpreter"/>
                    <field name="patient_id" string="Claimant"/>
                    <field name="location_id" />
                    <field name="cancel_reason_id" invisible="1"/>
                    <field name="created_by" invisible="1"/>
                    <field name="company_id" groups="base.group_multi_company" />
                    <field name="state" />
                    <field name="event_type" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                    <button type="object" name="view_event_billing_form" icon="fa-check-circle" groups="bista_iugroup.group_record_manager"/>
                </tree>
            </field>
        </record>

        <record id="view_event_form" model="ir.ui.view">
            <field name="name">event.form</field>
            <field name="model">event</field>
            <field eval="1" name="priority"/>
            <field name="arch" type="xml">
                <form string="Events"  delete="false" quick_create="false">
                    <header>
                        <button string="Confirm" name="confirm_event" states="allocated" type="object" class="oe_highlight" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Authorize" name="confirm_event" states="unauthorize" type="object" class="oe_highlight" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Enter Timesheet" name="enter_timesheet" states="confirmed" type="object" class="oe_highlight" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="View Timesheet" name="view_timesheet" states="unbilled,invoiced" type="object" class="oe_highlight" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Reschedule" name="reschedule_event" states="scheduled,allocated,confirmed,cancel" type="object" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Mark As Done" name="mark_as_done" states="unbilled" type="object" invisible="1" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Cancel Event" name="cancel_event" type="object" attrs="{'invisible': [('state','in',('invoiced','cancel','done'))]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Follow Up" name="follow_up" type="object"/>
                        <button string="Recurring" type="action" name="5664" icon="fa-check-circle" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Upload Attachment" name="upload_attachment" type="object" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                        <button string="Update Interpreters" name="assign_computed_fields" type="object"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,scheduled,confirmed,unbilled,done"/>
                    </header>
                    <group>
                        <group>
                            <h3>
                                <field name="name" string="Event Id" readonly="1"/>
                            </h3>
                        </group>
                        <group colsapn="4" col="6">
                            <field name="event_type" string="Event Type" readonly="1"/>
                            <button type="action" name="5665" attrs="{'invisible': ['|',('state','in',('cancel','done')),('fee_note_test','=',True)]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter" string="Upload Fee Note" context="{'fee_note1':True}"/>
                            <button type="action" name="5665" attrs="{'invisible': ['|',('state','in',('cancel','done')),('order_note_test','=',True)]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter" string="Upload SAF" context="{'order_note1':True}"/>
                            <field name="attachment_present" invisible="1"/>
                            <field name="fee_note_test" invisible="1"/>
                            <field name="order_note_test" invisible="1"/>
                            <field name="customer_basis" invisible="1"/>
                            <field name="invoice_state" invisible="1"/>
                            <field name="customer_group" invisible="1"/>
                        </group>
                    </group>
                    <group>
                        <table>
                            <tr>
                                <font color="red">
                                    <label string="NOTE: Timesheet has been attached to this event" attrs="{'invisible': [('attachment_present','=', False)]}"/>
                                </font>
                            </tr>
                        </table>
                    </group>
                    <group colspan="4" col="6">
                        <group cols="6" colspan="2">
                            <notebook style="width:168px">
                                <page string="Billing Info">
                                    <div class="oe_list_content">
                                        <group>
                                            <field name="partner_id" style="width:168px" required="1" domain="[('customer', '=', 1),('is_company', '=',0),('cust_type', '=', 'customer'),('company_id', '=',company_id)]" string="Billing Customer" options="{'limit': 7, 'create': false, 'create_edit': false ,'always_reload': True}" context="{ 'show_address': 1,'form_view_ref':'bista_iugroup.view_partner_form_inherit_iu'}" attrs="{'readonly':[('state','in',('invoiced','cancel','done'))]}"/>
                                            <field name="contact_id" style="width:168px" domain="[('customer', '=', 1),('cust_type', '=', 'contact'),('is_company', '=',0),('company_id','=',company_id)]" context="{ 'form_view_ref':'bista_iugroup.view_contact_form','search_default_cust_type':'contact','default_cust_type':'contact','default_is_company':0,                                                 'default_customer':1,'default_supplier':0,'default_company_id': company_id}" string="Billing Contact" attrs="{'readonly':[('state','in',('invoiced','cancel','done'))]}"/>
                                        </group>
                                        <group>
                                            <group>
                                                <field name="emergency_rate"/>
                                            </group>
                                            <group>
                                                <field name="event_id" string="Old Event ID" readonly="1"/>
                                            </group>
                                        </group>
                                        <group>
                                            <group>
                                                <field name="is_follow_up" readonly="1"/>
                                            </group>
                                        </group>
                                        <h3>
                                            <group>
                                                <field name="patient_id"  style="color: #5F9EA0; width:168px" domain="[('company_id','=',company_id)]" context="{'default_company_id': company_id}" attrs="{'readonly':[('state','in',('cancel'))]}"/>

                                                <field name="ref" string="Reference" style="width:168px" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))],'invisible':[('ordering_partner_id','=',177990)]}"/>
                                            </group>
                                        </h3>
                                        <group>
                                            <field name="project_name_id" attrs="{'readonly':[('state','in',('cancel'))],'required': [('customer_group','in',('project_ref_required'))]}" style="color: #5F9EA0; width:168px" context="{'default_company_id': company_id}"/>
                                        </group>
                                    </div>
                                    <group invisible="1">
                                        <field name="employer" placeholder="Employer Name" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                    </group>
                                    <group>
                                        <field name="zone_id" style="width:168px" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}" domain="[('company_id','=',company_id)]" options="{'limit': 7, 'create': false, 'create_edit': false}"/>
                                        <field name="suppress_email"/>

                                    </group>
                                    <group>
                                        <field name="company_id" style="width:168px" groups="base.group_multi_company" widget="selection" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                    </group>
                                    <group>
                                        <field name="language_id" style="width:168px" context="{ 'show_group': 1}" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))],'required': ['|',('event_type','=','language')],'invisible': [('event_type','=','transport')]}" options="{'limit': 7, 'create': false, 'create_edit': false}" domain="[('company_id','=',company_id),('active_custom','=',True)]"/>
                                    </group>
                                    <group>
                                        <field name="mental_prog" style="width:168px" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                        <field name="medical_no" style="width:168px" attrs="{'readonly':[('state','in',('cancel','done'))],'required': [('customer_group','in',('kaiser','mrn_required','mrn_dept_reqiured','choc'))]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>

                                        <field name="special_discount" style="width:168px" string="Spc. Discount" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                        <field name="po_no" style="width:168px" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>

                                    </group>
                                    <group>
                                        <field name="user_id" domain="[('company_id','=',company_id)]" options="{'limit': 7, 'create': false, 'create_edit': false}"/>
                                        <field name="task_id" style="width:168px" readonly="1"/>
                                        <field name="scheduler_id" style="width:168px" domain="[('company_id','=',company_id),('user_type','=','staff')]" options="{'limit': 7, 'create': false, 'create_edit': false}" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}" required="1"/>
                                        <field name="sales_representative_id" string="Sales Rep." readonly="1"/>
                                        <field name="km" string="Distance" invisible="1"/>
                                    </group>
                                    <group>
                                        <group>
                                            <field name="cust_invoice_id" attrs="{'invisible': [('cust_invoice_id','=',0)]}" readonly="1" groups="bista_iugroup.group_record_manager"/>
                                        </group>
                                        <group>
                                            <field name="view_interpreter_inv" attrs="{'invisible': ['|','|',('event_type','=','transport'),('view_interpreter_inv','=',0)]}" groups="bista_iugroup.group_record_manager"/>
                                        </group>
                                    </group>
                                    <group>
                                        <field name="view_interpreter_inv2" attrs="{'invisible': ['|','|',('event_type','=','transport'),('multi_type','=','1'),('view_interpreter_inv2','=',0)]}" groups="bista_iugroup.group_record_manager"/>
                                        <field name="view_interpreter_inv3" attrs="{'invisible': ['|','|',('event_type','=','transport'),('multi_type','in',['1','2']),('view_interpreter_inv3','=',0)]}" groups="bista_iugroup.group_record_manager"/>
                                    </group>
                                    <group>
                                        <field name="view_interpreter_inv4" attrs="{'invisible': ['|','|',('event_type','=','transport'),('multi_type','in',['1','2','3']),('view_interpreter_inv4','=',0)]}" groups="bista_iugroup.group_record_manager"/>
                                        <field name="view_interpreter_inv5" attrs="{'invisible': ['|','|',('event_type','=','transport'),('multi_type','in',['1','2','3','4']),('view_interpreter_inv5','=',0)]}" groups="bista_iugroup.group_record_manager"/>
                                    </group>
                                    <button name="import_interpreter_new" string="Search Interpreter" type="object" attrs="{'invisible': ['|',('event_type','in',('transport','translation')),('state','in',('cancel','done'))]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                                    <separator string="List of Interpreters" attrs="{'invisible': ['|',('event_type','not in',('language'))]}"/>
                                    <group>
                                        <h4 style="color: #5F9EA0;"> Blue: Preferred Interpreters, Green: visited Interpreters</h4>
                                    </group>
                                    <div class="adv_search" style="width:325px">
                                        <button name="clear_interpreter_list" string="Clear List" type="object"/>
                                        <button name="job_offer_multiple_interpreters" string="Offer Job" type="object"/>
                                        <div style="width:359px">
                                        <field name="interpreter_ids" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))],'invisible': ['|',('event_type','not in',('language')),('state','in',('cancel','done'))]}" colspan="2" widget="one2many_list" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter" options="{'reload_on_button': true}">
                                            <tree  string="List of Interpreters" editable="top" create="false" decoration-info="preferred == True" decoration-success="visited == True">
                                                <field name="interpreter_id" readonly="1" options="{&quot;no_open&quot;: true}"/>
                                                <field name="preferred" readonly="1" string="pref" invisible="1"/>
                                                <field name="visited" readonly="1"/>
                                                <field name="phone" readonly="1"/>
                                                <field name="rate" readonly="1"/>
                                                <field name="duration" readonly="1" invisible="1"/>
                                                <field name="distance" readonly="1"/>
                                                <button name="onclick_select_interpreter" string="Select" icon="fa-check-circle" type="object"/>
                                                <field name="select_interpreter" readonly="1"/>
                                                <field name="parent_state" invisible="1"/><!-- icon="terp-stock_zoom" -->
                                                <button name="get_direction" type="object" icon="marker" attrs="{'invisible': [('parent_state','in',('done','cancel'))]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                                                <button string="Offer Job" name="leave_voicemail" type="object" icon="fa-arrow-right" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter" attrs="{'invisible': [('parent_state','in',('done','cancel'))]}" invisible="1"/>
                                                <button type="action" name="5660" icon="fa-check" attrs="{'invisible': ['|',('parent_state','in',('done','cancel')),('state','in',('cancel'))]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter" invisible="1"/>
                                                <field name="state" invisible="1"/>
                                            </tree>
                                        </field>
                                            </div>
                                    </div>
                                    <button name="import_transporter_new" string="Search Transporters" type="object" attrs="{'invisible': ['|',('event_type','in',('language','translation')),('state','in',('cancel','done'))]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter"/>
                                    <separator string="List of Transporters" attrs="{'invisible': ['|',('event_type','=','language')]}"/>
                                    <div class="adv_search">
                                        <field name="transporter_ids" readonly="1" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))],'invisible': ['|',('event_type','=','language')]}" options="{'reload_on_button': true}" colspan="2" widget="one2many_list" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter">
                                            <tree string="List of Transporters" editable="top" delete="false" create="false" decoration-success="visited == True">
                                                <field name="transporter_id" readonly="1"/>
                                                <field name="visited" readonly="1"/>
                                                <field name="phone" readonly="1" invisible="1"/>
                                                <field name="rate" readonly="1"/>
                                                <field name="duration" readonly="1"/>
                                                <field name="distance" readonly="1"/>
                                                <field name="parent_state" invisible="1"/>
                                                <button type="action" name="5662" attrs="{'invisible': ['|',('parent_state','in',('done','cancel')),('state','in',('cancel'))]}" groups="bista_iugroup.group_record_manager,bista_iugroup.group_iu_cust_service,bista_iugroup.group_iu_recruiter" icon="fa-check"/>
                                                <field name="state" invisible="1"/>
                                            </tree>
                                        </field>
                                    </div>
                                </page>
                            </notebook>
                        </group>
                        <group cols="4" colspan="2">
                            <notebook>
                                <page string="Order Info">
                                    <div class="oe_list_content">
                                        <group>
                                            <field name="ordering_partner_id" cols="2" colspan="2" required="1" domain="[('customer', '=', 1),('cust_type', '=', 'customer'),('is_company', '=',0),('company_id','=',company_id)]" context="{ 'show_address': 1,'form_view_ref':'bista_iugroup.view_partner_form_inherit_iu','default_customer':1,'default_supplier':0,'default_is_company':0, 'default_cust_type':'customer','default_company_id': company_id}" attrs="{'readonly':[('state','in',('invoiced','cancel','done'))]}"/>
                                            <field name="ordering_contact_id" cols="2" colspan="2" domain="[('customer', '=', 1),('cust_type', '=', 'contact'),('is_company', '=',0),('company_id','=',company_id)]" string="Ordering Contact Id" context="{ 'form_view_ref':'bista_iugroup.view_contact_form'}" options="{'limit': 7, 'create': false, 'create_edit': false}" attrs="{'readonly':[('state','in',('invoiced','cancel','done'))]}" required="1"/>
                                        </group>
                                    </div>
                                    <group colspan='2'>
                                        <field name="org_number" attrs="{'invisible':[('ordering_partner_id','!=',177990)]}"/>
                                        <field name="event_start_date" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                    </group>
                                    <div class="address_format" cols="6" colspan="2">
                                        <label for="event_start_hr" style="width: 42%" string="Start Time"/>
                                        <field name="event_start_hr" style="width:13%" class="custom_character" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}" context="{'field':'event_start_hr'}"/>
                                        : <field name="event_start_min" style="width: 13%" class="custom_character" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}" context="{'field':'event_start_min'}"/>
                                        :<field name="am_pm" style="width: 15%" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                        <label for="event_end_hr" style="width: 42%" string="Timezone"/>
                                        <field name="customer_timezone" groups="bista_iugroup.group_iu_admin" style="width: 46%" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                    </div>
                                    <div class="address_format" cols="6" colspan="2">
                                        <label for="event_end_hr" style="width: 42%" string="End Time"/>
                                        <field name="event_end_hr" style="width: 13%" class="custom_character" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}" context="{'field':'event_end_hr'}"/>
                                        : <field name="event_end_min" class="custom_character" style="width: 13%" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}" context="{'field':'event_end_min'}"/>
                                        :<field name="am_pm2" style="width: 15%" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}"/>
                                    </div>
                                    <group >
                                        <field name="event_start" readonly="1" groups="bista_iugroup.group_iu_admin"/>
                                        <field name="event_end" readonly="1" groups="bista_iugroup.group_iu_admin"/>
                                        <field name="actual_event_start" readonly="1" groups="bista_iugroup.group_iu_admin"/>
                                        <field name="actual_event_end" readonly="1" groups="bista_iugroup.group_iu_admin"/>
                                    </group>
                                    <group>
                                        <field name="transportation_type" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))],'invisible': ['|',('event_type','in',('language'))],                                                  'required': [('state','in',('draft','scheduled','allocated')),('event_type','=','transport')]}"/>
                                        <field name="appointment_type_id" options="{'limit': 7, 'create': false, 'create_edit': false}" attrs="{'readonly':[('state','in',('cancel'))]}"/>
                                        <field name="certification_level_id" options="{'limit': 7, 'create': false, 'create_edit': false}" attrs="{'readonly':[('state','in',('confirmed','unbilled','invoiced','cancel','done'))]}" domain="[('company_id','=',company_id)]"/>
                                        <field name="event_purpose" attrs="{'readonly':['|',('state','in',('cancel','done')),('invoice_state','not in',('draft',False))]}"/>
