        <record id="view_event_filter" model="ir.ui.view">
            <field name="name">event.search</field>
            <field name="model">event</field>
            <field name="arch" type="xml">
                <search string="Events">
                    <field name="name" string="Event Id"/>
                    <field name="event_id" string="Old Event Id"/>
                    <field name="ref" string="Reference" />
                    <filter name="my_events" help="My Events" icon="fa-user+" domain="[('scheduler_id','=',uid)]"/>
                    <filter help="My created Events" icon="fa-user" domain="[('user_id','=',uid)]"/>
                    <field name="partner_id" string="Billing Customer"/>
                    <field name="ordering_partner_id" string="Ordering Customer"/>
                    <field name="language_id"/>
                    <field name="doctor_id" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                    <field name="zone_id" invisible="1"/>
                    <field name="all_interpreter_email" string="Interpreter Email"/>
                    <field name="interpreters_phone" string="Interpreter Phone"/>
                    <filter name="my_zone" string="My Zone Events" domain="[]" context="{'zone': True}"/>
                    <filter string="Unscheduled" icon="fa-file-o" domain="[('state','=','draft')]" help = "Unscheduled"/>
                    <filter string="Job Offered" icon="fa-check" domain="[('state', '=' ,'scheduled')]" help = "Job Offered"/>
                    <filter string="Scheduled" icon="fa-pause" domain="[('state','=','allocated')]" help = "Scheduled"/>
                    <filter string="Confirmed" icon="fa-pause" domain="[('state','=','confirmed')]" help = "Confirmed"/>
                    <filter string="unbilled" icon="fa-pause" domainview_event_form="[('state','=','unbilled')]" help = "Unbilled"/>
                    <filter string="invoiced" icon="fa-pause" domain="[('state','=','invoiced')]" help = "Invoiced"/>
                    <filter string="Cancelled" icon="fa-times" name="cancel" domain="[('state','=','cancel')]"/>
                    <filter string="Done" icon="fa-times" name="done" domain="[('state','=','done')]"/>
                    <filter name="today" string="Today" domain="[('event_date','=',time.strftime('%%Y-%%m-%%d'))]"/>
                    <filter icon="fa-file-o" name="representative" string="Sale Representative" domain="[('sales_representative_id.user_id','=',uid)]" help="Event that belongs to the sale representative"/>
                    <filter icon="fa-file-o" name="authorized" string="Authorized" domain="[('is_authorized','=',True)]"/>
                    <filter icon="fa-file-o" name="unauthorized" string="Unauthorized" domain="[('is_authorized','=',False)]"/>
                    <filter string="Staff Events" name="staff_event" icon="fa-user" domain="[('created_by','not in',('customer',('contact')))]" />
                    <filter string="Portal Events" name="portal_events" icon="fa-user" domain="[('created_by','in',('customer',('contact')))]" />
                    <filter name="past_events" string="Past Events" domain="[('event_end','&lt;=', time.strftime('%%Y-%%m-%%d %%H:%%M:%%S'))]"/>
                    <filter name="present_future_events" string="Today &amp; Future Events" domain="[('event_start','&gt;=', time.strftime('%%Y-%%m-%%d %%H:%%M:%%S'))]"/>
                    <filter name="latest_days" string="Last 365 Days" domain="[('event_start_date','&lt;=', time.strftime('%%Y-%%m-%%d')),('event_start_date','&gt;=',(context_today()- datetime.timedelta(days=365)).strftime('%%Y-%%m-%%d'))]"/>
                    <separator/>
                    <field name="patient_id" string="Patient Name" />
                    <field name="translator_id"/>
                    <field name="transporter_id"/>
                    <field name="scheduler_id"/>
                    <field name="cancel_reason_id" />
                    <field name="event_start_date"/>
                    <group expand="0" string="Group By...">
                        <filter string="Salesperson" icon="fa-user" domain="[]" context="{'group_by' : 'user_id'}" />
                        <filter string="Translator" name="Translator" icon="fa-user" context="{'group_by':'translator_id'}" />
                        <filter string="Transporter" name="Transporter" icon="fa-user" context="{'group_by':'transporter_id'}" />
                        <filter string="Scheduler" name="Scheduler" icon="fa-user" context="{'group_by':'scheduler_id'}" />
                        <filter string="Patient" name="patient" icon="fa-user" context="{'group_by':'patient_id'}" />
                        <filter string="Billing Customer" name="customer" icon="fa-user" context="{'group_by':'partner_id'}" />
                        <field string="Language" name="language_id" icon="fa-user" context="{'group_by':'language_id'}"/>
                        <filter string="Month" icon="fa-calendar-o" context="{'group_by':'month'}" help="Month"/>
                        <filter string="Year" icon="fa-calendar" context="{'group_by':'year'}" help="Year"/>
                        <filter string="Company" icon="fa-user" domain="[]" context="{'group_by' : 'company_id'}" />
                        <field string="Created User Type" name="created_by" icon="fa-user" context="{'group_by':'created_by'}"/>
                    </group>
                </search>
            </field>
        </record>

