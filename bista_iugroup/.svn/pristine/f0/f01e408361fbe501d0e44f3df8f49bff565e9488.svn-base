        <record id="event_approval" model="mail.template">
            <field name="name">Event Approval </field>
            <field name="model_id" ref="bista_iugroup.model_event"/>
            <field name="email_from" >donotreply@iugroup.com</field>
            <field name="email_to" >${(object.approving_mgr_email or '')|safe}</field>
            <field name="subject">Event ${object.name or ''} is Waiting Approval</field>
            <field name="body_html"><![CDATA[
                 <div style="font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; ">
                    <p>Hello, ${object.name or ''} is waiting approval.</p>
                    <br/>
                    <table>
                        <tr>
                            <td>Event No:</td>
                            <td>${object.name or ''}</td>
                        </tr>
                        <tr>
                    		<td>Ordering Contact : </td>
                    		<td>${(object.ordering_contact_id and object.ordering_contact_id.complete_name)|safe}</td>
                    	</tr>
                        <tr>
                            <td>Event Date:</td>
                            <td>${(object.event_start_date or ' - - ').split('-')[1]+ '/' + (object.event_start_date or ' - - ').split('-')[2] + '/' + (object.event_start_date or ' - - ').split('-')[0]} at ${object.event_start_hr or ''}:${object.event_start_min or ''} ${str(object.am_pm or '').upper()}
                                    To ${object.event_end_hr or ''}:${object.event_end_min or ''} ${str(object.am_pm2 or '').upper()}</td>
                        </tr>
                        <tr>
                            <td>Language:</td>
                            <td>${object.language_id.name or ''}</td>
                        </tr>
                        <tr>
                            <td>Location:</td>
                            <td>${object.location_id.street or ''}
                            <br/>${object.location_id.city or ''}
                            <br/>${object.location_id.state_id.name or ''}
                            <br/>${object.location_id.country_id.name or ''}
                            <br/>${object.location_id.zip or ''}</td>
                        </tr>
                    </table>
                    <p>Please do not hesitate to contact us if you have any questions.</p>
                    <br/><br/><br/>
                </div>
                <p>${(object.user_id.name or '')|safe}<br/>
                </p>
                <a href="https://erp.iugroup.com${(object.approval_url or '')|safe}" ><h3>Click here to Approve or Reject the Event.</h3></a>
            ]]>
            </field>
        </record>
        
